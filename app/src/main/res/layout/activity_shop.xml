<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity_shop">


    <ImageView
        android:id="@+id/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.738"
        app:srcCompat="@drawable/white" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="match_parent"
        android:layout_height="120dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/tree" />

    <ImageView
        android:id="@+id/save"
        android:layout_width="match_parent"
        android:layout_height="78dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/shopping" />

    <ImageView
        android:id="@+id/cosmos"
        android:layout_width="186dp"
        android:layout_height="194dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tomatokit"
        app:layout_constraintVertical_bias="0.3"
        app:srcCompat="@drawable/cosmos" />

    <TextView
        android:id="@+id/tomatokit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="토마토 키트"
        android:textColor="#568A35"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.224"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tomato"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/cosmoskit"
        android:layout_width="94dp"
        android:layout_height="19dp"
        android:text="코스모스 키트"
        android:textColor="#568A35"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.208"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cosmos"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/tomato"
        android:layout_width="186dp"
        android:layout_height="194dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/save"
        app:layout_constraintVertical_bias="0.097"
        app:srcCompat="@drawable/tomato" />

    <ImageView
        android:id="@+id/sunflower"
        android:layout_width="186dp"
        android:layout_height="194dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/save"
        app:layout_constraintVertical_bias="0.1"
        app:srcCompat="@drawable/sunflower" />

    <TextView
        android:id="@+id/sunflowerkit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="해바라기 키트"
        android:textColor="#568A35"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.778"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/sunflower"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/beankit"
        android:layout_width="87dp"
        android:layout_height="21dp"
        android:gravity="center"
        android:text="강낭콩 키트"
        android:textColor="#568A35"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.774"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/bean"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/bean"
        android:layout_width="186dp"
        android:layout_height="194dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.892"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/sunflowerkit"
        app:layout_constraintVertical_bias="0.3"
        app:srcCompat="@drawable/bean" />


    <ImageView
        android:id="@+id/toshop"
        android:layout_width="61dp"
        android:layout_height="59dp"
        app:layout_constraintTop_toBottomOf="@id/cosmoskit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.045"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.714"
        app:srcCompat="@drawable/back" />

</androidx.constraintlayout.widget.ConstraintLayout>